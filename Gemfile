source 'https://rubygems.org'

gem 'rails'
gem 'puma'
gem 'pg'

# Gems used only for assets and not required
# in production environments by default.
group :assets do
  gem 'sass-rails'
  gem 'coffee-rails'

  gem 'uglifier'
end

gem 'jquery-rails'
gem 'haml'

gem 'nested_set'

gem 'quiet_assets', :group => :development